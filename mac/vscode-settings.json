{
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 5000,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.codeActionsOnSave": {},
  "editor.inlineSuggest.enabled": true,
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "redhat.telemetry.enabled": true,
  "python.formatting.provider": "black",
  "workbench.colorTheme": "Noctis Bordo",
  "editor.tokenColorCustomizations": {},
  "editor.stickyScroll.enabled": true,
  "editor.language.colorizedBracketPairs": [],
  "html.format.unformatted": "",
  "background.fullscreen": {
    // "image": "https://wallpapers.com/images/hd/sekiro-shadows-die-twice-4-6xktdkb4movogxib-6xktdkb4movogxib.jpg", // url of your image
    "image": [
      "https://wallpapers.com/images/hd/sekiro-shadows-die-twice-4-6xktdkb4movogxib-6xktdkb4movogxib.jpg",
      "https://i.redd.it/3dttftxq8po91.png"
    ], // An array may be useful when set interval for carousel
    "opacity": 0.94, // 0.85 ~ 0.95 recommended
    "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
    "interval": 120 // seconds of interval for carousel, default `0` to disabled.
  }
}
